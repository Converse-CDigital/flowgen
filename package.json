{
  "name": "flowgen",
  "description": "Generate flowtype definition files from TypeScript",
  "typings": "index.d.ts",
  "version": "1.11.0",
  "bin": {
    "flowgen": "./lib/cli/index.js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts",
      "!**/*.d.ts"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.1",
    "@babel/highlight": "^7.10.1",
    "commander": "^5.1.0",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "typescript": "^3.9.5",
    "typescript-compiler": "^1.4.1-2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "flow-bin": "^0.102.0",
    "jest": "^26.0.1"
  },
  "files": [
    "lib",
    "index.d.ts",
    "index.js.flow"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "scripts": {
    "prettier": "prettier  --write \"{src,types}/**/*.ts\"",
    "prepublishOnly": "npm run compile",
    "compile": "babel ./src --out-dir lib --delete-dir-on-start --ignore 'src/**/*.spec.ts'",
    "compile:watch": "npm run compile -- -w",
    "test": "jest",
    "lint": "eslint '**/*.js' --quiet"
  }
}
